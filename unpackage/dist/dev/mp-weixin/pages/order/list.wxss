page {
  background-color: #FFFFFF;
}
.zaiui-sort-grid-box {
  position: relative;
  margin: 27rpx 0;
  width: 100%;
}
.zaiui-sort-grid-box .scroll-view {
  position: relative;
  white-space: nowrap;
  width: 100%;
}
.zaiui-sort-grid-box .scroll-view .scroll-item {
  display: inline-block;
  position: relative;
  text-align: center;
  margin: 0 21.81rpx;
  width: 127rpx;
}
.zaiui-sort-grid-box .scroll-view .scroll-item:first-child {
  margin-left: 18.18rpx;
}
.zaiui-sort-grid-box .scroll-view .scroll-item:last-child {
  margin-right: 18.18rpx;
}
.zaiui-sort-grid-box .scroll-view .scroll-item .cu-avatar {
  width: 99.99rpx;
  height: 99.99rpx;
  border-radius: 50%;
}
.zaiui-sort-grid-box .scroll-view .scroll-item .text {
  position: relative;
  padding: 14.54rpx 18.18rpx 0;
  width: 100%;
}
.zaiui-nav-tab-view {
  position: -webkit-sticky;
  position: sticky;
  padding: 2rpx 0 9rpx;
  z-index: 9999;




  top: calc(25px + 161rpx);

  -webkit-transition: all .25s;
  transition: all .25s;
}
.zaiui-nav-tab-view .nav.z .cu-item.select .tab-dot {
  height: 5.45rpx;
}
.zaiui-order-tab-view {
  position: relative;
  margin: 0 27rpx;
  margin-top: 27rpx;
  border-radius: 9rpx;
  padding: 27rpx 18.18rpx;
  box-shadow: 0 0 14.54rpx #f1f1f1;
}
.zaiui-order-tab-view:last-child {
  margin-bottom: 27rpx;
}
.zaiui-order-tab-view .shop-title-view {
  position: relative;
  margin-bottom: 27rpx;
}
.zaiui-order-tab-view .shop-title-view .cu-avatar {
  position: absolute;
}
.zaiui-order-tab-view .shop-title-view .shop-name {
  position: relative;
  height: 47.27rpx;
  line-height: 47.27rpx;
  padding: 0 199.99rpx 0 61.81rpx;
}
.zaiui-order-tab-view .shop-title-view .text-right {
  position: absolute;
  top: 7.27rpx;
  right: 0;
}
.zaiui-order-tab-view .goods-list-view {
  position: relative;
  margin-top: 27rpx;
}
.zaiui-order-tab-view .goods-list-view .cu-avatar {
  position: absolute;
  width: 158.18rpx;
  height: 158.18rpx;
}
.zaiui-order-tab-view .goods-list-view .goods-info-view {
  position: relative;
  padding-left: 176.36rpx;
  height: 158.18rpx;
}
.zaiui-order-tab-view .goods-list-view .goods-info-view .name {
  position: relative;
  height: 36.36rpx;
}
.zaiui-order-tab-view .goods-list-view .goods-info-view .introduce {
  position: relative;
  height: 32.72rpx;
  margin-top: 9rpx;
}
.zaiui-order-tab-view .goods-list-view .goods-info-view .tag-view {
  position: relative;
  height: 30.9rpx;
  margin-top: 9rpx;
}
.zaiui-order-tab-view .goods-list-view .goods-info-view .tag-view .cu-tag {
  position: relative;
  top: -3.63rpx;
}
.zaiui-order-tab-view .goods-list-view .goods-info-view .text-price {
  height: 30.9rpx;
  margin-top: 9rpx;
}
.zaiui-order-tab-view .statistics-view {
  position: relative;
  padding: 18.18rpx 0;
  margin-top: 18.18rpx;
  border-bottom: 2rpx solid #f3f3f3;
}
.zaiui-order-tab-view .zaiui-btn-view {
  position: relative;
  text-align: right;
  margin-top: 18.18rpx;
}
.zaiui-order-tab-view .zaiui-btn-view .cu-btn {
  background: #FFFFFF;
}
.zaiui-order-tab-view .zaiui-btn-view .cu-btn + .cu-btn {
  margin-left: 9rpx;
}
.zaiui-foot-tip-view {
  position: relative;
  text-align: center;
  margin-top: 72.72rpx;
  margin: 27rpx 0;
}
.zaiui-foot-tip-view .img-view {
  position: relative;
  margin-bottom: 9rpx;
}
.zaiui-foot-tip-view .img-view .cu-avatar {
  background: #FFFFFF;
  background-size: cover;
  background-position: center;
}
.zaiui-null-view {
  position: relative;
  text-align: center;
  margin-top: 72.72rpx;
}
.zaiui-null-view .img-view {
  position: relative;
  margin-bottom: 9rpx;
}
.zaiui-null-view .img-view .cu-avatar {
  width: 363.63rpx;
  height: 363.63rpx;
  background: #FFFFFF;
  background-size: cover;
  background-position: center;
}

