page {
  background: #FFFFFF;
}
.zaiui-sponsored-card-view {
  position: relative;
  margin: 27rpx 27rpx 0;
}
.zaiui-sponsored-card-view .card-price-view {
  position: relative;
  background: #FFF5F5;
  border-radius: 14.54rpx 14.54rpx 0 0;
  padding: 18.18rpx;
}
.zaiui-sponsored-card-view .card-price-view .price-left-view {
  position: absolute;
  height: 125.45rpx;
  width: 145.45rpx;
  text-align: center;
  line-height: 125.45rpx;
}
.zaiui-sponsored-card-view .card-price-view .price-left-view .price {
  font-size: 45.45rpx;
  font-weight: 400;
}
.zaiui-sponsored-card-view .card-price-view .name-content-view {
  position: relative;
  padding-left: 163.63rpx;
  padding-right: 145.45rpx;
  height: 125.45rpx;
  line-height: 1.8;
  color: #999898;
}
.zaiui-sponsored-card-view .card-price-view .name-content-view::before {
  content: '';
  position: absolute;
  top: -18.18rpx;
  bottom: -18.18rpx;
  margin-left: -18.18rpx;
  border-left: 2rpx dashed #fdbabc;
}
.zaiui-sponsored-card-view .card-price-view .btn-right-view {
  position: absolute;
  right: 27rpx;
  top: 18.18rpx;
  height: 125.45rpx;
  line-height: 125.45rpx;
}
.zaiui-sponsored-card-view .card-num-view {
  position: relative;
  background: #FFECED;
  border-radius: 0 0 14.54rpx 14.54rpx;
  border-top: 2rpx dashed #dedbdb;
  padding: 10.9rpx 27rpx;
  color: #999898;
}
.zaiui-sponsored-card-view .card-num-view::before {
  content: '';
  position: absolute;
  width: 36.36rpx;
  height: 36.36rpx;
  background: #ffffff;
  border-radius: 50%;
  top: -18.18rpx;
  left: -18.18rpx;
}
.zaiui-sponsored-card-view .card-num-view::after {
  content: '';
  position: absolute;
  width: 36.36rpx;
  height: 36.36rpx;
  background: #ffffff;
  border-radius: 50%;
  top: -18.18rpx;
  right: -18.18rpx;
}
.zaiui-sponsored-card-view .card-num-view view {
  position: relative;
  padding-right: 72.72rpx;
}
.zaiui-sponsored-card-view .card-num-view text {
  position: absolute;
  right: 27rpx;
  top: 14.54rpx;
}

