page {
  background: #FFFFFF;
}
.zaiui-title {
  margin: 36.36rpx 27rpx;
}
.zaiui-right-text text {
  position: relative;
}
.zaiui-right-text text + text {
  margin-left: 18.18rpx;
}
.zaiui-right-text text + text:after {
  content: ' ';
  position: absolute;
  width: 2rpx;
  height: 18.18rpx;
  border-left: 2rpx solid;
  top: 3.63rpx;
  left: -9rpx;
}
.content-view-box {
  position: relative;
}
.content-view-box .font-view {
  padding: 27rpx;
  line-height: 1.7;
}
.content-view-box image {
  width: 100%;
}
.zaiui-hight-view {
  width: 100%;
  height: 36.36rpx;
  background: #FAFAFA;
}
.solid-bottom::after {
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
}
.zaiui-view-box {
  position: relative;
  padding: 18.18rpx 27rpx;
}
.zaiui-view-box .flex .basis-1 {
  -webkit-flex-basis: 10%;
          flex-basis: 10%;
}
.zaiui-view-box .flex .basis-9 {
  -webkit-flex-basis: 90%;
          flex-basis: 90%;
}
.zaiui-comment-box .zaiui-border-view {
  position: relative;
  background: #efebeb;
  margin: 18.18rpx 27rpx;
  height: 2rpx;
}
.zaiui-comment-box .basis-9 {
  padding-left: 18.18rpx;
}
.zaiui-comment-box .tag-text {
  position: relative;
}
.zaiui-comment-box .tag-text text {
  position: relative;
}
.zaiui-comment-box .tag-text text + text {
  margin-left: 18.18rpx;
}
.zaiui-comment-box .tag-text text + text:after {
  content: ' ';
  position: absolute;
  width: 2rpx;
  height: 18.18rpx;
  border-left: 2rpx solid;
  top: 3.63rpx;
  left: -9rpx;
}
.zaiui-comment-box .tools-icon-view .cuIcon-comment:before,
.zaiui-comment-box .tools-icon-view .cuIcon-appreciate:before {
  position: relative;
  margin-right: 9rpx;
  font-size: 40rpx;
  top: 3.63rpx;
}
.zaiui-comment-box .tools-icon-view text + text {
  margin-left: 36.36rpx;
}
.zaiui-footer-fixed {
  box-shadow: 0 -3.63rpx 10.9rpx 0 #eaeaea;
}
.zaiui-footer-fixed .flex-direction {
  padding: 18.18rpx 27rpx;
}
.foot-hight-view {
  width: 100%;
  height: 154rpx;
  background: #FAFAFA;
}

