.zaiui-view-box {
  position: relative;
  padding: 0 18.18rpx;
}
.zaiui-view-box .zaiui-card {
  position: relative;
  padding: 18.18rpx;
  border-radius: 9rpx;
  box-shadow: 0 0 10.9rpx #f7f7f7;
}
.address-view .cu-list.menu-avatar > .cu-item {
  height: 55px;
}
.address-view .cu-list.menu-avatar > .cu-item:after {
  width: 0;
  height: 0;
  border-bottom: 0;
}
.address-view .cu-list.menu-avatar > .cu-item .icon-view {
  position: absolute;
  border-radius: 100%;
  text-align: center;
  line-height: 45.45rpx;
  height: 45.45rpx;
  width: 45.45rpx;
  left: 9rpx;
  top: 20rpx;
}
.address-view .cu-list.menu-avatar > .cu-item .content {
  left: 72.72rpx;
  width: calc(100% - 94rpx - 27rpx);
}
.address-view .cu-list.menu-avatar > .cu-item .action {
  text-align: right;
  width: 36.36rpx;
}
.address-view .address-line {
  position: absolute;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
  border-radius: 0 0 9rpx;
  height: 9rpx;
  width: 100%;
  bottom: 0;
  right: 0;
  left: 0;
}
.goods-view .title-view {
  position: relative;
  line-height: 49rpx;
  height: 47.27rpx;
}
.goods-view .title-view .cu-avatar {
  position: absolute;
}
.goods-view .title-view .title-box {
  position: relative;
  padding-left: 63.63rpx;
}
.goods-view .title-view .title-box .cu-tag {
  position: relative;
  top: -3.63rpx;
}
.goods-view .goods-info-view-box {
  position: relative;
  padding: 27rpx 18.18rpx;
}
.goods-view .goods-info-view-box .cu-avatar {
  position: absolute;
  height: 163.63rpx;
  width: 163.63rpx;
  left: 18.18rpx;
}
.goods-view .goods-info-view-box .goods-info-view {
  position: relative;
  padding-left: 181rpx;
  height: 163.63rpx;
  line-height: 1.4;
}
.goods-view .goods-info-view-box .goods-info-view .zaiui-tag-view {
  position: relative;
}
.goods-view .goods-info-view-box .goods-info-view .goods-price-view {
  position: relative;
  line-height: 1.7;
  top: 9rpx;
}
.goods-view .solid-bottom::after {
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
}
.goods-view .zaiui-gift-list {
  position: relative;
}
.goods-view .zaiui-gift-list .title {
  padding-right: 181rpx;
}
.goods-view .zaiui-gift-list .text-right {
  position: absolute;
  right: 0;
  top: 0;
}
.goods-view .zaiui-gift-list + .zaiui-gift-list {
  margin-top: 10px;
}
.goods-view .checked-view {
  position: relative;
}
.goods-view .checked-view .zaiui-checked {
  position: absolute;
  -webkit-transform: scale(0.7);
          transform: scale(0.7);
}
.goods-view .checked-view .text-black-view {
  padding-left: 63.63rpx;
  line-height: 47.27rpx;
}
.goods-view .zaiui-service-view .flex-sub {
  position: relative;
  text-align: center;
  line-height: 1.8;
  border: 2rpx solid #f7f5f5;
}
.goods-view .zaiui-service-view .flex-sub .cu-tag {
  position: relative;
  top: -4rpx;
}
.goods-view .zaiui-service-view .flex-sub .zaiui-corner-mark {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 0;
  height: 0;
  border-bottom: 36.36rpx solid #eaeaea;
  border-left: 45.45rpx solid transparent;
}
.goods-view .zaiui-service-view .flex-sub .check-icon {
  position: absolute;
  font-size: 20rpx;
  bottom: -3px;
  color: #FAFAFA;
  right: 2rpx;
}
.goods-view .zaiui-service-view .flex-sub .zaiui-corner-mark.check {
  border-bottom: 36.36rpx solid #e54d42;
}
.goods-view .zaiui-service-view .flex-sub .check-icon.check {
  color: #fff;
}
.zaiui-card.pay-view {
  position: relative;
  padding: 10px 10px 0;
}
.zaiui-card.pay-view .title-view {
  position: relative;
}
.zaiui-card.pay-view .title-view .text-right {
  position: absolute;
  right: 0;
}
.zaiui-card.pay-view .tag-view-box {
  position: relative;
  padding: 0 0 27rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}
.zaiui-card.pay-view .tag-view-box .cu-tag {
  margin-top: 18.18rpx;
  margin-right: 9rpx;
}
.zaiui-card.pay-view .tag-view-box .cu-tag + .cu-tag {
  margin-left: 0;
}
.zaiui-card.pay-view .cu-list.menu > .cu-item {
  padding: 0;
  min-height: 90.9rpx;
}
.zaiui-card.pay-view .cu-list.menu > .cu-item .content {
  font-size: 27rpx;
}
.zaiui-card.pay-view .cu-list.menu > .cu-item .action .cu-tag {
  position: relative;
  top: -3.63rpx;
  right: 9rpx;
}
.zaiui-card.pay-view .cu-list.menu > .cu-item.arrow {
  padding-right: 29rpx;
}
.zaiui-card.pay-view .cu-list.menu > .cu-item.arrow:before {
  right: 0;
  color: #aaaaaa;
}
.zaiui-price-view {
  position: relative;
}
.zaiui-price-view .item-view {
  position: relative;
}
.zaiui-price-view .item-view .title {
  padding-right: 181rpx;
}
.zaiui-price-view .item-view .text-right {
  position: absolute;
  right: 0;
  top: 0;
}
.zaiui-price-view .item-view + .item-view {
  margin-top: 18.18rpx;
}
.agreement-checked-view {
  position: relative;
  padding: 18.18rpx 27rpx;
}
.agreement-checked-view .zaiui-checked {
  position: absolute;
  -webkit-transform: scale(0.7);
          transform: scale(0.7);
}
.agreement-checked-view .text-black-view {
  padding-left: 63.63rpx;
  line-height: 47.27rpx;
}
.zaiui-footer-fixed {
  box-shadow: 0 -2rpx 9rpx 0 #ececec;
  z-index: 99999;
}
.zaiui-footer-fixed .cu-bar {
  width: 100%;
}
.zaiui-footer-fixed .cu-bar .price-view {
  width: 100%;
  padding-right: 254.54rpx;
}
.zaiui-footer-fixed .cu-bar .btn-view {
  position: absolute;
  float: right;
  right: 29rpx;
}

